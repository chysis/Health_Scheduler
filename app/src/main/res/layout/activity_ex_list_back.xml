<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ex_list_back">


    <TextView
        android:id="@+id/chooseplace"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@android:color/holo_blue_light"
        android:gravity="center"
        android:text="헬스장 - 등 운동"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addedList"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_marginTop="124dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="담은 리스트"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <HorizontalScrollView
        android:id="@+id/horizontalScrollView"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.414">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/btnBenchPull"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:padding="0dp"
                android:scaleType="centerCrop"
                android:src="@drawable/bench_pull"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txtBenchPull"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:text="Bench Pull"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/btnBenchPull"
                app:layout_constraintStart_toStartOf="@+id/btnBenchPull"
                app:layout_constraintTop_toBottomOf="@+id/btnBenchPull" />

            <ImageButton
                android:id="@+id/btnBentOverBarbellRow"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:padding="0dp"
                android:scaleType="centerCrop"
                android:src="@drawable/bent_over_barbelll_row"
                app:layout_constraintStart_toEndOf="@+id/btnBenchPull"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/txtBentOverBarbellRow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:text="Bent Over\nBarbell Row"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/btnBentOverBarbellRow"
                app:layout_constraintStart_toStartOf="@+id/btnBentOverBarbellRow"
                app:layout_constraintTop_toBottomOf="@+id/btnBentOverBarbellRow" />


            <ImageButton
                android:id="@+id/btnChinUp"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:padding="0dp"
                android:scaleType="centerCrop"
                android:src="@drawable/chin_up"
                app:layout_constraintStart_toEndOf="@+id/btnBentOverBarbellRow"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/txtChinUp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:text="Chin Up"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/btnChinUp"
                app:layout_constraintStart_toStartOf="@+id/btnChinUp"
                app:layout_constraintTop_toBottomOf="@+id/btnChinUp" />

            <ImageButton
                android:id="@+id/btnPullUp"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:padding="0dp"
                android:scaleType="centerCrop"
                android:src="@drawable/pull_up"
                app:layout_constraintStart_toEndOf="@+id/btnChinUp"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/txtPullUp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:text="Pull Up"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/btnPullUp"
                app:layout_constraintStart_toStartOf="@+id/btnPullUp"
                app:layout_constraintTop_toBottomOf="@+id/btnPullUp" />

            <ImageButton
                android:id="@+id/btnLatPulldown"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:padding="0dp"
                android:scaleType="centerCrop"
                android:src="@drawable/lat_pull_down"
                app:layout_constraintStart_toEndOf="@+id/btnPullUp"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txtLatPulldown"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:text="Lat Pulldown"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/btnLatPulldown"
                app:layout_constraintStart_toStartOf="@+id/btnLatPulldown"
                app:layout_constraintTop_toBottomOf="@+id/btnLatPulldown" />

            <ImageButton
                android:id="@+id/btnSeatedRow"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:padding="0dp"
                android:scaleType="centerCrop"
                android:src="@drawable/seated_row"
                app:layout_constraintStart_toEndOf="@+id/btnLatPulldown"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txtSeatedRow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:text="Seated Row"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/btnSeatedRow"
                app:layout_constraintStart_toStartOf="@+id/btnSeatedRow"
                app:layout_constraintTop_toBottomOf="@+id/btnSeatedRow" />

            <ImageButton
                android:id="@+id/btnDbRow"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:padding="0dp"
                android:scaleType="centerCrop"
                android:src="@drawable/bent_over_dumbbell_row"
                app:layout_constraintStart_toEndOf="@+id/btnSeatedRow"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txtDbRow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:text="Dumbbell Row"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/btnDbRow"
                app:layout_constraintStart_toStartOf="@+id/btnDbRow"
                app:layout_constraintTop_toBottomOf="@+id/btnDbRow" />








        </androidx.constraintlayout.widget.ConstraintLayout>


    </HorizontalScrollView>







</androidx.constraintlayout.widget.ConstraintLayout>